<?xml version="1.0" encoding="utf-8"?>
<style class="in-text" version="1.0" and="symbol" et-al-min="5" et-al-use-first="1" initialize="false" initialize-with="" name-as-sort-order="first" demote-non-dropping-particle="never" xmlns="http://purl.org/net/xbiblio/csl">
  <info>
    <title>wagwan</title>
    <id>http://www.zotero.org/styles/apa</id>
    <author>
      <name>Ashley M Price</name>
    </author>
    <updated>2024-03-30T23:11:28+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term name="editor">
        <single>ed.</single>
        <multiple>eds.</multiple>
      </term>
      <term name="translator">
        <single>trans.</single>
        <multiple>trans.</multiple>
      </term>
    </terms>
  </locale>
  <macro name="cunt-sort">
    <names variable="author">
      <name name-as-sort-order="all" sort-separator=", " delimiter=", "/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
      <label prefix=" (" suffix="). "/>
    </names>
  </macro>
  <macro name="cunt">
    <names variable="author" delimiter=", " suffix=". ">
      <name/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
      </substitute>
      <label prefix=" (" suffix="). "/>
    </names>
  </macro>
  <macro name="cunt-count">
    <names variable="author">
      <name form="count"/>
        <substitute>
          <names variable="editor"/>
          <names variable="translator"/>
        </substitute>
      </names>
    </macro>
  <citation et-al-min="3" et-al-use-first="1" disambiguate-add-year-suffix="true" disambiguate-add-names="true" disambiguate-add-givenname="true" collapse="year" givenname-disambiguation-rule="primary-name-with-initials">
    <layout delimiter="; " prefix="(" suffix=")">
      <group>
        <names variable="author" delimiter=", "/>
      </group>
    </layout>
  </citation>
  <bibliography hanging-indent="true" name-as-sort-order="first" et-al-min="21" et-al-use-first="19" entry-spacing="0" line-spacing="1" and="text" initialize-with=" ">
    <sort>
      <key macro="cunt-sort" sort="ascending" names-min="1" names-use-first="1"/>
      <key macro="cunt-count" names-min="3" names-use-first="3"/>
      <key macro="cunt-sort" sort="ascending" names-min="3" names-use-first="1"/>
      <key variable="issued" sort="ascending"/>
    </sort>
    <layout>
      <group>
        <choose>
          <if type="book" match="any">
            <choose>
              <if match="all" variable="author editor translator">
                <names variable="author" delimiter=", " suffix=". ">
                  <name/>
                </names>
                <names variable="translator" prefix="Translated " suffix=". ">
                  <name/>
                </names>
                <names variable="editor" prefix="Edited " delimiter=", " suffix=". ">
                  <name/>
                </names>
              </if>
              <else-if match="all" variable="author editor">
                <names variable="author" delimited=", " suffix=". ">
                  <name/>
                </names>
                <names variable="editor" prefix="Edited " delimiter=", " suffix=". ">
                  <name/>
                </names>
              </else-if>
              <else-if match="all" variable="author translator">
                <names variable="author" delimited=", " suffix=". ">
                  <name/>
                </names>
                <names variable="translator" prefix="Translated " delimiter=", " suffix=". ">
                  <name/>
                </names>
              </else-if>
              <else-if match="all" variable="editor translator">
                <names variable="editor" delimited=", " suffix=". ">
                  <name/>
                  <label/>
                </names>
                <names variable="translator" prefix="Translated " delimiter=", " suffix=". ">
                  <name/>
                </names>
              </else-if>
              <else>
                <names variable="author" delimiter=", " suffix=". ">
                  <name/>
                  <substitute>
                    <names variable="editor"/>
                    <names variable="translator"/>
                  </substitute>
                  <label prefix=" (" suffix=")"/>
                </names>
              </else>
            </choose>
            <date date-parts="year" form="text" variable="issued" suffix=". "/>
            <text variable="title" strip-period="true" font-style="italic" suffix=". "/>
            <text variable="publisher-place" form="short" suffix=": "/>
            <text variable="publisher" suffix="."/>
          </if>
          <else-if type="thesis" match="any">
            <names variable="author" delimiter=", " suffix=". "/>
            <date date-parts="year" form="text" variables="issued" suffix=". "/>
            <text variable="title" strip-periods="true" suffix=". "/>
            <text variable="genre" strip-periods="false" suffix=". "/>
            <text variable="publisher-place" form="short" suffix=": "/>
            <text variable="publisher" suffix="."/>
          </else-if>
          <else-if type="article-journal" match="any">
            <names variable="author" delimiter=", " suffix=". "/>
            <date date-parts="year" form="text" variable="issued" suffix=". "/>
            <text variable="title" suffix=". "/>
            <text variable="container-title" font-style="italic"/>
            <choose>
              <if match="any" variable="volume">
                <text variable="volume" prefix=" "/>
                <choose>
                  <if match="any" variable="issue">
                    <text variable="issue" prefix=", "/>
                    <choose>
                      <if match="any" variable="page"><text variable="page" prefix=", " suffix="."/></if>
                      <else><text value="."/></else>
                    </choose>
                  </if>
                  <else-if match="any" variable="page"><text variable="page" prefix=", " suffix="."/></else-if>
                  <else><text value="."/></else>
                </choose>
              </if>
              <else-if match="any" variable="issue">
                <text variable="issue" prefix=" "/>
                <choose>
                  <if match="any" variable="page"><text variable="page" prefix=", " suffix="."/></if>
                  <else><text value="."/></else>
                </choose>
              </else-if>
              <else>
                <choose>
                  <if match="any" variable="page"><text variable="page" prefix=" " suffix="."/></if>
                  <else><text value="."/></else>
                </choose>
              </else>
            </choose>
          </else-if>
        </choose>
      </group>
    </layout>
  </bibliography>
</style>
